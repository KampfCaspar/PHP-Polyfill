{
  "name": "kampfcaspar/polyfill",
  "description": "all those little utils the language is missing",
  "homepage": "https://github.com/KampfCaspar/PHP-Polyfill/",
  "license": "AGPL-3.0-or-later",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan-phpunit": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "KampfCaspar\\Polyfill\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KampfCaspar\\Test\\Polyfill\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "KampfCaspar",
      "email": "code@kampfcaspar.ch",
      "role": "Developer"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}